<h1>Customer Data</h1>

<table>
    <tr>
        <td>id</td>
        <td>Name</td>
        <td>Email</td>
    </tr>
    <% customers.forEach(function(item) { %>
    <tr>
        <td><%=item.id %></td>
        <td><%=item.name %></td>
        <td><%=item.email %></td>
    </tr>
    <% }); %>
</table>